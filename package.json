{
  "name": "deepnest.net",
  "version": "1.0.0",
  "description": "deepnest.net website",
  "type": "module",
  "repository": "https://github.com/deepnest-next/deepnest.net.git",
  "author": "Josef Froehle <github@josef-froehle.de>",
  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
		"build": "npm-run-all clean:site build:11ty",
		"build:11ty": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/",
    "clean:site":"rimraf -rf _site",
		"start": "npm-run-all clean:site start:11ty",
		"start:11ty": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --pathprefix=/eleventy-base-blog/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-fetch": "^5.0.2",
    "@11ty/eleventy-img": "^6.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@aaashur/eleventy-plugin-add-remote-data": "^1.0.0",
    "@aloskutov/eleventy-plugin-external-links": "^2.1.1",
    "@pcdevil/eleventy-plugin-intl-utils": "^1.1.0",
    "@sardine/eleventy-plugin-inline-svg": "^0.0.1",
    "cross-env": "^7.0.3",
    "eleventy-plugin-automatic-noopener": "^2.0.2",
    "eleventy-plugin-edit-on-github": "^1.1.0",
    "eleventy-plugin-embed-everything": "^1.20.0",
    "eleventy-plugin-footnotes": "^0.11.0",
    "eleventy-plugin-github-repos": "^0.0.2",
    "eleventy-plugin-i18n": "^0.1.3",
    "eleventy-plugin-link_to": "^0.4.1",
    "eleventy-plugin-metagen": "^1.8.3",
    "eleventy-plugin-mtos": "^0.2.0",
    "eleventy-plugin-purgecss": "^0.5.0",
    "eleventy-plugin-sitemap": "^1.0.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "rosetta": "^1.1.0",
    "slugify": "^1.6.6",
    "stylelint": "^16.14.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "luxon": "^3.5.0",
    "prismjs": "^1.29.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.1"
  },
  "dependencies": {
    "@zachleat/heading-anchors": "^1.0.1",
    "postcss": "^8.5.3"
  }
}