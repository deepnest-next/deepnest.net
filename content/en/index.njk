      <p id="lightbox-caption" class="text-white text-xl"></p>
      <p class="text-gray-400 text-sm mt-2">Click outside the image or press ESC to close</p>
    </div>
  </div>
</div>

<!-- Lightbox Script -->
<script>
  let currentImagePath = '';
  // Store reference to the original container
  let lightboxImgContainer = null;

  function openLightbox(imgSrc, caption) {
    const lightbox = document.getElementById('lightbox');
    const lightboxCaption = document.getElementById('lightbox-caption');

    // Get the image container on first use or if it's not available
    if (!lightboxImgContainer) {
      lightboxImgContainer = document.querySelector('#lightbox .overflow-hidden.rounded-lg.shadow-2xl');
    }

    // Get the template with the given ID
    const template = document.getElementById(imgSrc);

    if (template && template.tagName === 'SCRIPT') {
      // Use the template HTML content
      const templateContainer = document.createElement('div');
      templateContainer.innerHTML = template.textContent.trim();

      // Clear the container and prepare for new content
      lightboxImgContainer.innerHTML = '';

      // Get the picture element from the template
      const picture = templateContainer.querySelector('picture');
      if (picture) {
        // Add appropriate classes to the responsive image
        const img = picture.querySelector('img');
        if (img) {
          img.className = 'w-full max-h-[80vh] object-contain bg-black/60 transition-all duration-300';
          img.removeAttribute('height');
        }

        // Add the picture element to the container
        lightboxImgContainer.appendChild(picture);

        // Show lightbox with animation
        lightbox.classList.remove('opacity-0', 'pointer-events-none');
        lightbox.querySelector('.max-w-6xl').classList.remove('scale-95');
        lightbox.querySelector('.max-w-6xl').classList.add('scale-100');
        lightboxCaption.textContent = caption;
        document.body.style.overflow = 'hidden';

        // Get the path for "open in new window" feature
        const imgElement = picture.querySelector('img');
        if (imgElement) {
          currentImagePath = imgElement.src;
        }
      }
    }
  }

  function openInNewWindow() {
    // Open the current image in a new browser window
    if (currentImagePath) {
      window.open(currentImagePath, '_blank');
    }
  }

  function closeLightbox() {
    const lightbox = document.getElementById('lightbox');

    // Hide with animation
    lightbox.classList.add('opacity-0');
    lightbox.querySelector('.max-w-6xl').classList.remove('scale-100');
    lightbox.querySelector('.max-w-6xl').classList.add('scale-95');

    // After animation completes, disable pointer events
    setTimeout(() => {
      lightbox.classList.add('pointer-events-none');
      document.body.style.overflow = 'auto';
    }, 300);
  }

  // Close lightbox when clicking outside the content
  document.getElementById('lightbox').addEventListener('click', function(e) {
    if (e.target === this) {
      closeLightbox();
    }
  });

  // Close lightbox with Escape key
  document.addEventListener('keydown', function(e) {
    if (e.key === 'Escape') {
      closeLightbox();
    }
  });
</script>

